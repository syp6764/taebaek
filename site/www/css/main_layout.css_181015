/* -- 관광 -- */
.colgroup2{}
.colgroup2 > div{transition:all 0.4s ease;}
.colgroup2 .rowgroup2_1 > div,.colgroup2 .rowgroup2_1 > section{transition:all 0.4s ease;}
.colgroup2.active .rowgroup2_1 > div,.colgroup2.active .rowgroup2_1 > section{transform:translate(0, 0);}
.colgroup2 .rowgroup2_2{transform:translate(1200px, 0);}
/* nature */
.colgroup2 .nature{width:450px;transform:translate(0, 600px);}
.colgroup2 .nature li{position:relative;overflow:hidden;float:left;width:50%;}
.colgroup2 .nature li a{display:block;height:185px;}
.colgroup2 .nature .sort01 a{background-image:url('../images/main/nature_bg1.jpg');}
.colgroup2 .nature .sort02 a{background-image:url('../images/main/nature_bg2.jpg');}
.colgroup2 .nature .sort03 a{background-image:url('../images/main/nature_bg3.jpg');}
.colgroup2 .nature .sort04 a{background-image:url('../images/main/nature_bg4.jpg');}
.colgroup2 .nature .curtain{display:block;position:absolute;left:50%;top:50%;width:300px;height:300px;margin:-150px 0 0 -150px;border-radius:100%;transform:scale(1, 1);transition:transform 400ms ease-in-out 0s;z-index:1;}
.colgroup2 .nature .sort01 .curtain{background-color:rgba(121,180,42,0.7);}
.colgroup2 .nature .sort02 .curtain{background-color:rgba(59,95,174,0.7);}
.colgroup2 .nature .sort03 .curtain{background-color:rgba(212,114,79,0.7);}
.colgroup2 .nature .sort04 .curtain{background-color:rgba(129,107,64,0.7);}
.colgroup2 .nature a:hover .curtain{transform:scale(0, 0);}
.colgroup2 .nature .title{display:block;position:absolute;left:0;top:65px;width:100%;text-align:center;z-index:2;}
.colgroup2 .nature .more{display:block;position:absolute;left:50%;bottom:40px;width:29px;height:29px;margin-left:-14px;background-image:url('../images/main/nature_more.png');z-index:2;}
/* sports */
.colgroup2 .sports{position:relative;background:url('../images/main/sports_pattern.gif') repeat;transform:translate(0, -370px);}
.colgroup2 .sports h2{padding:38px 0 0 38px;}
.colgroup2 .sports .inner{width:450px;height:370px;background:url('../images/main/sports_bg.png') no-repeat right top;}
.colgroup2 .sports ul{margin:30px 0 0 42px;}
.colgroup2 .sports li{position:relative;padding-left:10px;margin-top:7px;}
.colgroup2 .sports li:first-child{margin:0;}
.colgroup2 .sports li:before{position:absolute;left:0;top:13px;width:5px;height:1px;background-color:#fff;content:'';}
.colgroup2 .sports li a{font-size:17px;color:#fff;}
/* 축제행사 */
.colgroup2 .festival{position:relative;width:300px;padding-bottom:15px;background-color:#fbce01;transform:translate(0, 600px);}
.colgroup2 .festival h2{display:block;padding:30px 0 5px 30px;font-size:22px;color:#3f3a34;}
.colgroup2 .festival .slide_control{display:block;position:absolute;top:126px;left:0;width:100%;z-index:10;}
.colgroup2 .festival .slide_control button{display:block;width:18px;height:34px;padding:0;font-size:0;line-height:0;}
.colgroup2 .festival .slide_control .btn_prev{float:left;margin-left:26px;background-image:url('../images/main/festival_prev.png');}
.colgroup2 .festival .slide_control .btn_next{float:right;margin-right:26px;background-image:url('../images/main/festival_next.png');}
.colgroup2 .festival .slide_wrap{overflow:hidden;position:relative;z-index:1;width:260px;height:293px;margin:0 auto;}
.colgroup2 .festival .festival_list{overflow:hidden;position:relative;left:0;width:70000px;}
.colgroup2 .festival .festival_list li{float:left;width:260px;text-align:center;}
.colgroup2 .festival .list_cont{}
.colgroup2 .festival .list_cont .img_box{width:172px;height:167px;margin:0 auto;padding-top:5px;background:url('../images/main/festival_img_border.png') no-repeat 50%;}
