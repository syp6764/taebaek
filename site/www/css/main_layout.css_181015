.colgroup1 .rowgroup1_2{transform:translate(1200px, 0);}
/* 주요소식 */
.colgroup1 .board{position:relative;width:490px;height:220px;background-color:#fff;}
.colgroup1 .board .list{margin:0 30px;border-bottom:2px solid #c1c1c1;}
.colgroup1 .board .list > li{float:left;width:22.5%;}
.colgroup1 .board .tab_btn{display:block;position:relative;width:100%;height:50px;font-size:18px;line-height:50px;color:#636363;text-align:center;}
.colgroup1 .board .tab_btn i{display:none;position:absolute;left:0;bottom:-4px;width:100%;height:4px;background-color:#16186e;}
.colgroup1 .board .active .tab_btn{color:#2e3192;font-weight:600;}
.colgroup1 .board .active .tab_btn i{display:block;}
.colgroup1 .board .tabcontent{display:none;position:absolute;top:52px;left:0;width:100%;padding-top:24px;}
