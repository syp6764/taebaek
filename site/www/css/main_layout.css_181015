/* (주)한신정보기술 김예지 - 2017.07 */

/* ---------- 메인탭 ---------- */
.main_tab .col_box{height:660px;}
.main_tab .col_box .button_area{display:block;position:relative;overflow:visible;width:80px;height:68px;padding:0 6px;margin-right:-116px;margin-left:auto;z-index:13;font-size:18px;line-height:68px;color:#fff;/*transition:all 0.35s ease-in-out;*/}
.main_tab .col_box .button_area.on{width:80px;height:80px;padding:6px;margin:14px -116px 14px auto;}

.main_tab .col_box .button_area span{display:block;}

.main_tab .col_box .button_area.on span{position:relative;}
.main_tab .col_box .button_area.on span:before{display:block;position:absolute;top:0;left:0;z-index:-1;width:64px;height:64px;content:'';transition-property:border-color;transition-duration:0.5s;transition-timing-function:ease-in-out;animation-name:pulsate;animation-duration:1.5s;animation-timing-function:ease-in-out;animation-iteration-count:infinite;}
.main_tab .col_box .btn1.button_area{}
.main_tab .col_box .btn1.button_area.on span:before{border:2px solid rgba(65,99,176,1);}
.main_tab .col_box .btn2.button_area{}
.main_tab .col_box .btn2.button_area.on span:before{border:2px solid rgba(16,40,66,1);}
.main_tab .col_box .btn3.button_area{}
.main_tab .col_box .btn3.button_area.on span:before{border:2px solid rgba(34,70,109,1);}
@keyframes pulsate {
	0% {opacity:0;transform:scale(0.1, 0.1);}
	50% {opacity:1;transform:scale(1, 1);}
	100% {opacity:0;transform:scale(1.25, 1.25);}
}

.main_tab .col_box .btn1,.main_tab .col_box .btn1.on{margin-top:0;}
.main_tab .col_box .inner_box > li:first-child .btn_wrap{padding-top:215px;}
.main_tab .col_box .btn1 span{background-color:#4163b0;}
.main_tab .col_box .btn2 span{background-color:#102842;}
.main_tab .col_box .btn3{}
.main_tab .col_box .btn3 span{background-color:#22466d;}

.main_tab .col_box .button_area span em{display:block;transition:all 200ms linear;}
.main_tab .col_box .button_area:hover span em{animation-direction:alternate-reverse;animation-duration:2s;animation-iteration-count:infinite;animation-name:tab_txt_move;}

@keyframes tab_txt_move {
	0%{transform:translate(0,0);}
	25%{transform:translate(0,0);}
	40%{transform:translate(0,-5px);}
	50%{transform:translate(0,0);}
	60%{transform:translate(0,-5px);}
	75%{transform:translate(0,0);}
	100%{transform:translate(0,0);}
}

.main_tab .bg_box{position:relative;z-index:10;}
.main_tab .bg_box .tab_bg{display:none;position:absolute;left:0;top:0;width:100%;z-index:10;}
.main_tab .bg_box .active{z-index:11;}
.main_tab .bg_box .wrap{height:660px;}
.main_tab .bg_box .bg01{display:block;background:url('../images/main/tab1_bg.jpg') center top no-repeat;}
.main_tab .bg_box .bg01 .wrap{background:url('../images/main/tab1_bg.jpg') center top no-repeat;}
.main_tab .bg_box .bg02{background:url('../images/main/tab2_bg.jpg') center top no-repeat;}
.main_tab .bg_box .bg02 .wrap{background:url('../images/main/tab2_bg.jpg') center top no-repeat;}
.main_tab .bg_box .bg03{background:url('../images/main/tab3_bg.jpg') center top no-repeat;}
.main_tab .bg_box .bg03 .wrap{background:url('../images/main/tab3_bg.jpg') center top no-repeat;}

.main_tab .col_box .active{z-index:12;}
.main_tab .col_box .colgroup{display:none;overflow:hidden;position:absolute;left:0;top:0;width:100%;height:600px;margin:30px 0;z-index:11;}
.main_tab .col_box .colgroup.active{display:block;}
.main_tab .colgroup > div > div,.main_tab .colgroup > div > section{float:left;}
@media only screen and (max-width:1000px) {
	.main_tab .col_box{height:976px;}
	.main_tab .col_box > ul.inner_box{text-align:center;}
	.main_tab .col_box .inner_box > li{display:inline-block;}
	.main_tab .col_box .inner_box .btn_wrap{margin-top:24px;}
	.main_tab .col_box .inner_box > li:first-child .btn_wrap{padding:0;}
	.main_tab .col_box .button_area{width:68px;height:80px;padding:6px 0;margin:0 0 0 -4px;}
	.main_tab .col_box .button_area.on{margin:0 11px 0 11px;}
	
	.main_tab .bg_box .tab_bg{background-size:cover;}
	.main_tab .bg_box .wrap{height:976px;background:none !important;}

	.main_tab .col_box .colgroup{margin-top:128px;text-align:left;}
}
@media only screen and (max-width:640px) {
	.main_tab{position:relative;}
	.main_tab .bg_box{position:absolute;left:0;top:0;width:100%;height:100%;}
	.main_tab .bg_box .tab_bg{height:100%;}
	.main_tab .bg_box .wrap{height:100%;}
	.main_tab .col_box{height:auto;min-height:980px;padding-bottom:40px;}
	.main_tab .col_box > ul.inner_box{text-align:left;}
	.main_tab .col_box .inner_box > li{display:block;}
	.main_tab .col_box .inner_box .btn_wrap{position:absolute;left:0;top:0;width:100%;text-align:center;}
	.main_tab .col_box .button_area{display:inline-block;width:80px;padding:6px;margin:0;}
	.main_tab .col_box .button_area.on{margin:0;}
	.main_tab .col_box .button_area.btn1,.main_tab .col_box .button_area.btn1.on{margin-right:180px;}
	.main_tab .col_box .button_area.btn3,.main_tab .col_box .button_area.btn3.on{margin-left:180px;}
	.main_tab .col_box .colgroup{position:relative;margin:0;padding:125px 0 0;}
}

/* -- 메인 -- */
.colgroup1 > div{transition:all 0.4s ease;}
.colgroup1.active > div{transform:translate(0, 0);}
.colgroup1 .rowgroup1_1{transform:translate(-1200px, 0);}
/* 비주얼팝업 */
