.colgroup1 .board .list ul li a{display:block;margin-left:14px;padding:0;font-size:14px;color:#333;line-height:1.8em;text-align:left;white-space:nowrap;overflow:hidden;text-overflow: ellipsis;}
.colgroup1 .board .more{position:absolute;top:-36px;right:23px;width:20px;height:20px;font-size:0;line-height:0;}
.colgroup1 .board .more a{display:block;width:100%;height:100%;background:url('../images/main/board_more.png') no-repeat 50%;}
/* shortcut */
.colgroup1 .shortcut{width:270px;height:220px;background-color:#fbce01;}
.colgroup1 .shortcut ul{overflow:hidden;}
.colgroup1 .shortcut li{float:left;width:50%;border-left:1px solid #fde780;border-top:1px solid #fde780;margin:-1px 0 0 -1px;}
.colgroup1 .shortcut li a{display:block;position:relative;padding:70px 0 14px;font-size:17px;color:#463c0e;text-align:center;}
.colgroup1 .shortcut a i{display:block;position:absolute;left:50%;width:48px;margin-left:-24px;background-repeat:no-repeat;background-position:center top;}
.colgroup1 .shortcut .icon01{top:26px;height:29px;background-image:url('../images/main/shortcut_icon01.png');}
.colgroup1 .shortcut .icon02{top:22px;height:39px;background-image:url('../images/main/shortcut_icon02.png');}
.colgroup1 .shortcut .icon03{top:26px;height:33px;background-image:url('../images/main/shortcut_icon03.png');}
.colgroup1 .shortcut .icon04{top:21px;height:37px;background-image:url('../images/main/shortcut_icon04.png');}
/* 팝업존 */
.colgroup1 .popup{position:relative;width:440px;padding:10px 0;background-color:#fff;}
.colgroup1 .popup h2{position:absolute;left:30px;top:26px;font-size:20px;color:#000;font-weight:normal;}
