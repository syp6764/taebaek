.colgroup1 .popup .popup_control{display:block;position:absolute;left:30px;top:77px;width:70px;height:70px;border-left:1px solid #dadada;border-top:1px solid #dadada;z-index:10;}
.colgroup1 .popup .popup_control button,.colgroup1 .popup .popup_control a{display:block;float:left;width:35px;height:35px;margin:0;padding:0;border-right:1px solid #dadada;border-bottom:1px solid #dadada;background:url('../images/main/popup_control.png') no-repeat -61px 10px;font-size:0;line-height:0;}
.colgroup1 .popup .popup_control a{width:34px;height:34px;}
.colgroup1 .popup .popup_control .btn_prev{background-position:12px 10px;}
.colgroup1 .popup .popup_control .btn_next{background-position:-23px 10px;}
