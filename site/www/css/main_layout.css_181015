.colgroup1 .visual .visual_count span{display:inline-block;}
.colgroup1 .visual .visual_control{display:block;position:absolute;left:0;bottom:0;z-index:10;}
.colgroup1 .visual .visual_control button{display:block;float:left;width:50px;height:50px;margin:0;padding:0;border-right:1px solid #4e4e4d;background:rgba(0,0,0,0.6) url('../images/main/visual_control.png') no-repeat -31px 19px;font-size:0;line-height:0;}
.colgroup1 .visual .visual_control .btn_prev{background-position:19px 19px;}
.colgroup1 .visual .visual_control .btn_pause{background-position:-29px 19px;}
.colgroup1 .visual .visual_control .btn_play{background-position:-131px 19px;}
.colgroup1 .visual .visual_control .btn_next{background-position:-81px 19px;}
.colgroup1 .visual .visual_wrap{overflow:hidden;position:relative;z-index:1;width:890px;height:380px;}
.colgroup1 .visual .visual_list a{display:block;}
/* 열린시장실 */
.colgroup1 .mayor{width:310px;}
.colgroup1 .mayor > a{display:block; line-height:0;}
.colgroup1 .mayor .title_box{display:block;background:#2e3192 url('../images/main/mayor_bg.png') no-repeat right bottom;}
